<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>WicketRAD Archetype Created Web Application</display-name>
    <filter>
        <filter-name>WicketRADWebApplication</filter-name>
        <filter-class>
            org.apache.wicket.protocol.http.WicketFilter
        </filter-class>
        <init-param>
            <param-name>applicationClassName</param-name>
            <param-value>
                nl.spelberg.brandweer.App
            </param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>WicketRADWebApplication</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

</web-app>
